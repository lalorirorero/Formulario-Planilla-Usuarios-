{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html", "has": [ { "type": "host", "value": "*" } ], "continue": true },
    { "src": "/(.*)", "dest": "/index.html", "status": 200, "check": true }
  ]
}
